{
  "name": "toy-ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "gen": "node ./script/genNewComp/index.js",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "build:doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts && node script/copyDir.js"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "less": "^4.1.2",
    "prismjs": "^1.25.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-md": "^0.11.6",
    "vue-tsc": "^0.29.8"
  }
}
